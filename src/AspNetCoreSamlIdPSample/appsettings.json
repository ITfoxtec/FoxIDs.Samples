{
  "Logging": {
    "LogLevel": {
      "Default": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Settings": {
    "RelyingParties": [
      {
        // Azure app service domain
        "SpMetadata": "https://foxidsxxxx.azurewebsites.net/testcorp/dev/(aspnetcore_saml_idp_sample)/saml/spmetadata"
        /* or custom domain
          "SpMetadata": "https://foxidsxxxx.com/testcorp/dev/(aspnetcore_saml_idp_sample)/saml/spmetadata"
        */
        /* or local development
          "SpMetadata": "https://localhost:44330/testcorp/dev/(aspnetcore_saml_idp_sample)/saml/spmetadata"
        */
      }
    ]
  },
  "Saml2": {
    "Issuer": "urn:itfoxtec:idservice:samples:aspnetcoresamlidpsample",
    "SignatureAlgorithm": "http://www.w3.org/2001/04/xmldsig-more#rsa-sha256",
    "CertificateValidationMode": "None",
    "RevocationMode": "NoCheck"
  }
}
